<% for(var i = 0; i < people.length; i++) { %>
<div class="person" data-person='<%= JSON.stringify(people[i]) %>'>
  <div class="actions">
    <a href="#" class="edit">edit</a>
  </div>
  <%= people[i].first_name %> <%= people[i].last_name %>
  <blockquote>
    <%= people[i].address %>
  </blockquote>
</div>
<% } %>
